	"Supervisor": {
		"prefix": "supervisor",
		"body": [
			"defmodule ${moduleName} do",
			"\tuse Supervisor",
			"",
			"\tdef start_link(${args}) do",
			"\t\tSupervisor.start_link(__MODULE__, [${args}], name: __MODULE__)",
			"\tend",
			"",
			"\tdef init([${args}]) do",
			"\t\tchildren = []",
			"",
			"\t\tSupervisor.init(children, strategy: :one_for_one)",
			"\tend",
			"end"
		],
		"description": "Insert code for a OTP Supervisor"
	}
